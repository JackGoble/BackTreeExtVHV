{
  "name": "BackTree",
  "version": "1.0",
  "description": "Provides a tree view for browsing history",
  "manifest_version": 2,

  "icons": {
    "16": "tiny1.png"
  },

  "browser_action": {
      "default_popup": "popup.html"
  },

  "background": {
    "persistent": false,
    "scripts": [
      "navigation_collecter.js",
      "background.js"
    ]
  },


  "permissions": [
    "contextMenus"
  ]





}
